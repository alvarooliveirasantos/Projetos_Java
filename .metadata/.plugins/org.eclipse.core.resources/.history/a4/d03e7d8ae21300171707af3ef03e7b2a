package Principal;

import javax.persistence.Entity;

@Entity
@Table(name="teste")
public class Teste {

	int id;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	String nome;
}
